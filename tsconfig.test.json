{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": ".tmp/test-build",
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "target": "ES2020",
    "noEmit": false,
    "emitDeclarationOnly": false,
    "sourceMap": false,
    "allowImportingTsExtensions": false
  },
  "include": [
    ".astro/types.d.ts",
    "src/env.d.ts",
    "types/node-shim.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": []
}
